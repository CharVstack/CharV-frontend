    "react-hook-form": "7.40.0",
    "react-router-dom": "6.4.5",
    "swr": "2.0.0",
    "typescript": "4.9.4",
    "web-vitals": "3.1.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@hookform/devtools": "4.3.0",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@stoplight/prism-cli": "4.10.5",
    "@storybook/addon-a11y": "6.5.14",
    "@storybook/addon-actions": "6.5.14",
